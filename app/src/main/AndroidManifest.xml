<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.harry.finkers">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoAcionBar"></activity>
        <activity
            android:name=".SplashScreen.SplashScreen"
            android:theme="@style/AppTheme.NoAcionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".intro.IntroActivity"
            android:theme="@style/AppTheme.NoAcionBar"></activity>

        <activity
            android:name=".FragmentAccount.SingInActivity"
            android:theme="@style/AppTheme.NoAcionBar" />

        <activity
            android:name=".FragmentAccount.SignUpActivity"
            android:theme="@style/AppTheme.NoAcionBar" />

        <activity
            android:name=".ForgotActivity.ForgotFirstActivity"
            android:theme="@style/AppTheme.NoAcionBar" />

        <activity
            android:name=".ForgotActivity.ForgotSecondActivity"
            android:theme="@style/AppTheme.NoAcionBar" />

        <activity android:name=".FragmentAccount.UserAddData"
            android:theme="@style/AppTheme.NoAcionBar">

        </activity>
        <activity android:name=".FragmentHome.BrandDetail"
            android:theme="@style/AppTheme.NoAcionBar">

        </activity>

    </application>

</manifest>